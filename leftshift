#include <stdio.h>
int main() {
    int n,r,s,m;
    printf("Enter the number:\n");
    scanf("%d",&n);
    printf("Enter m:\n");
    scanf("%d",&m);
        for(int i=1;i<=m;i++)
            {
                r=n%10;
                s=n/10;
                if(r!=0)
                {
                    n=(10000*r)+s;
                }
                else
                {
                    n=s;
                }
            }
        printf("\n%d",n);
    }
       
	return 0;
}
}
