#include <stdio.h>
int main()
{
int n,r,s,l;
printf("Enter the number\n");
scanf("%d",&n);
printf("Enter no of leftshifts\n");
scanf("%d",&l);
 for(int i=1;i<=l;i++)
  {
   r=n%10;
   s=n/10;
   if(r!=0)
    {
     n=(10000*r)+s;
    }
     else
    {
     n=s;
    }
    }
 printf("\n%d",n);
}
return 0;
}
}
